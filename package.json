{
  "name": "nhsdashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint --fix ./app/**/*.{ts,tsx,js,jsx,json,md}",
    "format": "prettier --write ./app/**/*.{ts,tsx,js,jsx,json,md}",
    "drizzle-kit": "drizzle-kit",
    "preinstall": "npx only-allow pnpm",
    "start:prod": "pnpm drizzle-kit generate && pnpm drizzle-kit push && rm drizzle.config.ts && rm .env && rm -r /app/db && pnpm start"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@mjackson/form-data-parser": "^0.7.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@react-router/node": "^7.8.1",
    "@react-router/serve": "^7.8.1",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.40.1",
    "isbot": "^5.1.30",
    "lucide-react": "^0.479.0",
    "react": "^19.1.1",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router": "^7.8.1",
    "remix-auth": "^4.2.0",
    "remix-auth-form": "^3.0.0",
    "sharp": "^0.33.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@react-router/dev": "^7.8.1",
    "@shadcn/ui": "^0.0.4",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-router-devtools": "^1.1.10",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
